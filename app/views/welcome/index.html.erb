<h2>IP Address Management System (IPAMS) 14.11.0</h2>

<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<h3>Existing LANs</h3>
<% @lans.each do |lan| %>
  <div class="entry">
     <%= lan.lan_number %>
     <%= lan.lan_name %>
     <%= sanitize(lan.lan_description) %>  
  </div>
<% end %>
