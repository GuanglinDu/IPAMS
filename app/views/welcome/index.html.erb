<% provide(:title, "Home") %>

<div class="row">
  <% if policy(:welcome).tip? %>
    <p><strong>Please ask the admin to get authorized! </strong></p>
  <% end %>

  <div class="#sequence_chart">
    <div id="sequence"></div>
    <div id="chart" style="text-align:center">
      <div id="explanation" style="visibility:hidden">
        <p id="statistics"></p>
      </div>
    </div>
  </div>

  <script type="text/javascript">
    $(document).ready(loadData("drawSunburst"));
  </script>
</div>
