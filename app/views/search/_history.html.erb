<table class="table table-striped table-condensed">
<thead>
  <tr>
    <th>IP Address</th>
    <th>MAC Address</th>
    <th>Usage</th>
    <th>User Name</th>
    <th>Dept Name</th>
    <th>Office Phone</th>
    <th>Cell Phone</th>
    <th>Building</th>
    <th>Room</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Recycled At</th>
  </tr>
</thead>
<tbody id="main-table-body">
  <% history.each do |his| %>
    <tr>
      <td><%= find_address(his.address_id).ip %></td>
      <td><%= his.mac_address %></td>
      <td><%= his.usage %></td>
      <td><%= his.user_name %></td>
      <td><%= his.dept_name %></td>
      <td><%= his.office_phone %></td>
      <td><%= his.cell_phone %></td>
      <td><%= his.building %></td>
      <td><%= his.room %></td>
      <td><%= his.start_date %></td>
      <td><%= his.end_date %></td>
      <td><%= his.created_at.localtime %></td>
    </tr>
  <% end %>
</tbody>
</table>
